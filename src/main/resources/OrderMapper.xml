<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.threefour.homelearn.order.mapper.OrderMapper">
    <select id="findByImpUid" resultType="Order">
        select * from ORDERS where IMP_UID = #{impUid}
    </select>
    <insert id="save">
        insert into ORDERS values (#{impUid}, #{ordererId}, #{merchantUid}, now(), now())
    </insert>
</mapper>
